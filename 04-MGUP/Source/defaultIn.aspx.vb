    Private Sub Page_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        'Put user code to initialize the page here

        Dim qsPage As String = Me.Request.QueryString("page")
        Dim qsSelect As String = Me.Request.QueryString("select")

        'Me.pnlMenu.Controls.Add(Me.LoadControl("Controls/menuLeft.ascx"))
        'Me.lblTitleMenuLeft.Text = GetLabel("lblTitleMenuLeftB.Title")

        If qsPage <> "" Then
            Me.pnlBody.Controls.Add(Me.LoadControl("Controls/" & getControlName(qsPage)))
            If qsSelect <> "" Then
                Me.lblTitleBody.Text = GetFunctionPath(qsSelect)
            Else
                Me.lblTitleBody.Text = GetFunctionPath(qsPage)
            End If
        End If
    End Sub

    Private Function GetFunctionPath(ByVal FunctionCode As String, Optional ByVal str As String = "") As String
        Dim objFunction As _Function = objI.loadFunction(FunctionCode)
        If str = "" Then
            str = objFunction.FunctionName
        Else
            str = objFunction.FunctionName & " > " & str
        End If
        If DBToStr(objFunction.ParentCode) = "" Then
            Return str
        Else
            Return GetFunctionPath(objFunction.ParentCode, str) '& " > " & str
        End If
    End Function

